{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "AMSSConnection": "ENV_NOVARIS_CONNECTION_SRING",
    "StorageAccount": "ENV_NOVARIS_STORAGE_ACCOUNT"
  },
  "ApiSettings": {
    "JwtOptions": {
      "Secret": "ENV_NOVARIS_SECRET_KEY",
      "Issuer": "ENV_NOVARIS_ISSUER",
      "Audience": "ENV_AUDIENCE"
    }
  },
  "SmtpSettings": {
    "Host": "ENV_NOVARIS_SMTP_HOST",
    "Port": 587,
    "Username": "ENV_NOVARIS_SMTP_USERNAME",
    "Password": "ENV_NOVARIS_SMTP_PASSWORD",
    "SenderName": "ENV_NOVARIS_SMTP_SENDER_NAME",
    "SenderEmail": "ENV_NOVARIS_SMTP_SENDER_EMAIL"
  },
  "StripeSettings": {
    "SecretKey": "ENV_NOVARIS_STRIPE_SECRET_KEY"
  }
}
